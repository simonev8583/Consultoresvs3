@model IEnumerable<Consultoresvs3.Models.ReporteUsuario>

<div id="NombreProyecto">
    <h4 style="color:#231bef;font-weight:bold;">Proyecto</h4>
    <h5>@ViewBag.ProyectoId.Nombre</h5>
    <h4 style="color:#231bef;font-weight:bold;">Empresa</h4>
    <h5>@ViewBag.ProyectoId.Empresa.NombreEmpresa</h5>

</div>
<div class="table-responsive">
    <table class="table table-striped">
        <tr>

            <th>
                <h7>Fecha</h7>
            </th>
            <th>
                <h7>Empleado</h7>
            </th>
            <th>
                <h7>Servicio</h7>
            </th>
            <th>
                <h7>Horas Trabajadas</h7>
            </th>
            <th></th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>

                <td>
                    @Html.DisplayFor(modelItem => item.FechaReporte)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Usuario.Nombre)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Servicio.Nombre)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.HTrabajadas)
                </td>
            </tr>
        }
    </table>
</div>


