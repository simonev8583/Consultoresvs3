@model IEnumerable<Consultoresvs3.Models.ReporteUsuario>

<table class="table">
    <tr>
        <th>
            <h7>Fecha</h7>
        </th>
        <th>
            <h7>Proyecto</h7>
        </th>
        <th>
            <h7>Empresa</h7>
        </th>
        <th>
            <h7>Empleado</h7>
        </th>
        <th>
            <h7>Servicio</h7>
        </th>
        <th>
            <h7>Horas Trabajadas</h7>
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FechaReporte)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Proyecto.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Proyecto.Empresa.NombreEmpresa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Usuario.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Servicio.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HTrabajadas)
            </td>
            <td>
                @Html.ActionLink("Editar", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Detalles", "Details", new { id = item.Id }) |
                @Html.ActionLink("Eliminar", "Delete", new { id = item.Id })
            </td>
        </tr>
    }
</table>

